def search(arr,a):
    for i in range(len(arr)):
        if arr[i]==a:
            print(a," IS PRESENT IN STRING")
arr=[]
for i in range(5):
    i = int(input('Enter the elements of string: '+str()+" "))
    arr.append(i)
print(arr)
a = int(input('Enter number to search in the string: '))
search(arr,a)
